{
  "name": "vue3-antd-app",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "test": "vite --mode test",
    "build": "vite build",
    "preview": "vite preview",
    "test:unit": "vitest",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .eslintignore",
    "format": "prettier --write src/",
    "lint:lint-staged": "lint-staged",
    "prepare": "husky install",
    "prettier": "prettier --write  \"src/**/*.{js,json,tsx,css,less,scss,vue,html,md}\"",
    "lint:prettier": "prettier --write  \"src/**/*.{js,json,tsx,css,less,scss,vue,html,md}\"",
    "commit": "git add . && git cz",
    "commitlint": "commitlint --edit"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^7.0.1",
    "@iconify-json/ant-design": "^1.1.16",
    "@iconify-json/carbon": "^1.1.36",
    "@iconify-json/mdi": "^1.1.67",
    "@iconify/iconify": "^3.1.1",
    "@vueuse/core": "^10.11.0",
    "ant-design-vue": "4.x",
    "axios": "^1.7.2",
    "dayjs": "^1.11.11",
    "number-precision": "^1.6.0",
    "pinia": "^2.1.7",
    "vue": "^3.4.29",
    "vue-router": "^4.3.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@iconify/utils": "^2.1.29",
    "@rushstack/eslint-patch": "^1.8.0",
    "@unocss/preset-icons": "^0.61.5",
    "@unocss/preset-rem-to-px": "^0.61.5",
    "@unocss/preset-uno": "^0.61.5",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vitejs/plugin-vue-jsx": "^4.0.0",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/test-utils": "^2.4.6",
    "commitizen": "^4.3.0",
    "commitlint-config-cz": "^0.13.3",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^7.2.1",
    "eslint": "^8.57.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-vue": "^9.23.0",
    "husky": "^9.1.1",
    "jsdom": "^24.1.0",
    "less": "^4.2.0",
    "lint-staged": "^15.2.7",
    "pinia-plugin-persistedstate": "^3.2.1",
    "prettier": "^3.2.5",
    "typescript": "^5.5.4",
    "unocss": "^0.61.5",
    "unplugin-icons": "^0.19.0",
    "unplugin-vue-components": "^0.27.3",
    "vite": "^5.3.1",
    "vite-plugin-purge-icons": "^0.10.0",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-plugin-vue-devtools": "^7.3.1",
    "vitest": "^1.6.0",
    "vue-i18n": "^9.13.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint --config commitlint.config.cjs -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue}": [
      "prettier --write",
      "eslint --cache --fix",
      "git add"
    ],
    "*.{vue,css,less,scss}": [
      "prettier --write"
    ],
    "*.{json,html,yml,md}": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": ".cz-config.cjs"
    }
  }
}
